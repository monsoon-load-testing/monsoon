{
  "name": "monsoon-end-to-end",
  "version": "1.0.0",
  "author": "monsoon-load-testing",
  "bin": {
    "monsoon": "./bin/run"
  },
  "bugs": "https://github.com/monsoon-load-testing/monsoon/issues",
  "devDependencies": {
    "@aws-cdk/assertions": "1.132.0",
    "@oclif/dev-cli": "^1.26.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "1.132.0",
    "globby": "^10.0.2",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/src/aws/bin/aws.ts",
    "/src/aws/lib/aggregating-lambda.ts",
    "/src/aws/lib/aws-stack.ts",
    "/src/aws/lib/metronome-lambda.ts",
    "/src/aws/lib/starting_lambda.ts",
    "/src/aws/lib/timestream.ts",
    "/src/aws/lib/vpc.ts"
  ],
  "homepage": "https://github.com/monsoon-load-testing/monsoon",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "monsoon",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/monsoon-load-testing/monsoon",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cd src/aws && cdk synth"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "^1.132.0",
    "@aws-cdk/aws-ecs": "^1.132.0",
    "@aws-cdk/aws-iam": "^1.132.0",
    "@aws-cdk/aws-lambda": "^1.132.0",
    "@aws-cdk/aws-s3": "^1.132.0",
    "@aws-cdk/aws-timestream": "^1.132.0",
    "@aws-cdk/core": "1.132.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.4",
    "aws-sdk": "^2.1031.0",
    "cli-ux": "^5.5.1",
    "dotenv": "^10.0.0",
    "inquirer": "^8.2.0",
    "nanoid": "^3.1.30",
    "ora": "^5.4.0",
    "path": "^0.12.7",
    "puppeteer": "^10.4.0",
    "source-map-support": "^0.5.16",
    "express": "^4.17.1",
    "express-validator": "^6.13.0"
  },
  "contributors": [
